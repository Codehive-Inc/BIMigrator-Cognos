{{#each relationships}}
relationship {{id}}
	{{#if join_on_date_behavior}}
	joinOnDateBehavior: {{join_on_date_behavior}}
	{{/if}}
	fromColumn: '{{from_table}}'.{{from_column}}
	toColumn: '{{to_table}}'.{{to_column}}
{{#if cross_filter_behavior}}
	crossFilteringBehavior: bothDirections
{{/if}}
{{#if cardinality}}
	fromCardinality: one
{{/if}}
{{#if is_active}}
	isActive: {{is_active}}
{{/if}}

{{/each}}
