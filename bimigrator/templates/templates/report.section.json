{
  "name": "{{name}}",
  "displayName": "{{display_name}}",
  "filters": [
    {{#each filters}}{
      "filterType": "{{type}}",
      "target": {
        "table": "{{table}}",
        "column": "{{column}}"
      },
      "value": {{value}}
    }{{#unless @last}},{{/unless}}
    {{/each}}
  ],
  "config": {
    "objects": {
      "visuals": [
        {{#each visuals}}{
          "id": "{{id}}",
          "type": "{{type}}",
          "properties": {{properties}}
        }{{#unless @last}},{{/unless}}
        {{/each}}
      ]
    },
    "layout": {
      "width": {{layout.width}},
      "height": {{layout.height}},
      "displayOption": "{{layout.display_option}}"
    }
  }
}
