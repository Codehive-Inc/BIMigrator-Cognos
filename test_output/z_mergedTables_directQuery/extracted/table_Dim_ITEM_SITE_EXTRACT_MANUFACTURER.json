,\n        a.[UNIT_OF_MEASURE],\n        a.[DATE_CREATED],\n        a.[TAX_IND],\n        a.[UNIT_PRICE],\n        a.[TYPE_CODE],\n        a.[FUNCTIONAL_GROUP],\n        a.[MATERIAL_CLASS],\n        a.[STATUS],\n        a.[SITE_RANK],\n        a.[DATE_LAST_INVENTORIED],\n        a.[QTY_ON_ORDER],\n        a.[QTY_ON_HAND],\n        a.[QTY_AVAILABLE],\n        a.[MINIMUM],\n        a.[MAXIMUM],\n        a.[EOQ_QTY],\n        a.[QA_CODE],\n        a.[QA_LEVEL],\n        a.[CYCLE_IND],\n        a.[SHELF_LIFE_DATE],\n        a.[DATE_LAST_RECEIVED],\n        a.[FERC_CODE],\n        a.[PRIMARY_LOC],\n        a.[SECONDARY_LOC],\n        a.[EXTRA_LOCS],\n        a.[DATE_LAST_APPROVED],\n        a.[APPROVER_ID],\n        a.[APPROVER_BADGE_NUMBER],\n        a.[APPROVAL_SWITCH],\n        a.[QA_CERT1],\n        a.[QA_CERT2],\n        a.[QA_CERT3],\n        a.[QA_CERT4],\n        a.[VALUE_ON_HAND],\n        a.[PREVIOUS_ITEM_ID],\n        a.[QTY_ALLOCATED],\n        a.[CONSUMABLE],\n        a.[SHELF_LIFE],\n        a.[PREVENT_MAINTENANCE],\n        a.[CORPORATE_SWITCH],\n        a.[ITEM_OWNER],\n        a.[SAFETY_IND],\n        a.[UTC_IND],\n        a.[ITEM_DEPT],\n        a.[IN_PROCESS_HOLD],\n        a.[PIT],\n        a.[QTY_EXCESS],\n        a.[NEW_RANK],\n        a.[SUB_STATUS],\n        a.[STRATIFIED_TRANS_COUNT],\n        a.[STRATIFIED_DATE],\n        a.[TOLERANCE_AMOUNT],\n        a.[TOLERANCE_PERCENTAGE],\n        a.[SITE_LAST_UPDATED],\n        a.[QTY_ON_HOLD],\n        a.[ITEM_LAST_UPDATED],\n        a.[UNIT_OF_PROPERTY_NUMBER],\n        a.[UNIT_OF_PROPERTY_IND],\n        a.[DATE_LAST_ISSUED],\n        a.[DATE_LAST_RETURNED],\n        a.[CRITICAL_ITEM],\n        a.[REVIEW_DATE],\n        a.[STOP],\n        a.[SAP_IND],\n        b.[MFG_PART_NUMBER],\n        b.[MODEL_PART_IND],\n        b.[ACTIVITY_CODE],\n        b.[MFG_CODE],\n        b.[MFG_NAME],\n        b.[COMPRESSED_PART_NUMBER],\n        b.[FOSSIL_OR_NUCLEAR],\n        b.[SITE],\n        b.[MFG_PART_NUMBER],\n    