<?xml version="1.0" ?>
<report xmlns="http://developer.cognos.com/schemas/report/16.2/" useStyleVersion="11.6" expressionLocale="en-us">
  <drillBehavior/>
  <queries>
    <query name="Query1">
      <source>
        <model/>
      </source>
      <selection>
        <dataItem aggregate="none" rollupAggregate="none" name="CustomerID">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[CustomerID]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="3"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="1"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="none" rollupAggregate="none" name="CustomerName">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[CustomerName]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="3"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="0"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="none" rollupAggregate="none" name="Region">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[Region]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="3"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="1"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="total" name="EnergyUsage_kWh">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[EnergyUsage_kWh]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="1"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="2"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="total" name="TariffPer_kWh">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[TariffPer_kWh]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="2"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="2"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="total" name="TotalBill">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[TotalBill]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="2"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="2"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="none" rollupAggregate="none" name="PaymentStatus">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[PaymentStatus]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="3"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="1"/>
          </XMLAttributes>
        </dataItem>
        <dataItem aggregate="none" rollupAggregate="none" name="ServiceType">
          <expression>[C].[C_Data_Module_Energy_usage_data].[Sheet1].[ServiceType]</expression>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_dataType" value="3"/>
            <XMLAttribute output="no" name="RS_dataUsage" value="1"/>
          </XMLAttributes>
        </dataItem>
        <dataItem name="Total count">
          <expression>count([PaymentStatus])</expression>
        </dataItem>
        <dataItem name="High Consumption Flag">
          <expression>if ([EnergyUsage_kWh] &gt; 1500) then ('High') else ('Normal')</expression>
        </dataItem>
      </selection>
    </query>
  </queries>
</report>
