table MATERIAL_CHARGES


    column 'CHARGED_DATE'
        dataType: string
        summarizeBy: none
        sourceColumn: CHARGED_DATE

        

        annotation SummarizationSetBy = Automatic

        
    column 'TRANSACTION_TYPE'
        dataType: string
        summarizeBy: none
        sourceColumn: TRANSACTION_TYPE

        

        annotation SummarizationSetBy = Automatic

        



    partition 'MATERIAL_CHARGES' = m
        mode: import
        source = 
            	let
					Source = Sql.Database("REPLACE_WITH_YOUR_SERVER", "REPLACE_WITH_YOUR_DATABASE"),
					SQL = Value.NativeQuery(Source, "SELECT
    ""MATERIAL_CHARGES"".""SITE_NUMBER"" AS ""SITE_NUMBER"",
    ""MATERIAL_CHARGES"".""ITEM_NUMBER"" AS ""ITEM_NUMBER"",
    ""MATERIAL_CHARGES"".""CHARGED_DATE"" AS ""CHARGED_DATE"",
    ""MATERIAL_CHARGES"".""TRANSACTION_TYPE"" AS ""TRANSACTION_TYPE""
FROM
""Database_Layer"".""MATERIAL_CHARGES""
WHERE
    [SITE_NUMBER]= ?SiteNumber?
    AND     [ITEM_NUMBER]=?ItemNumber?
    AND     [TRANSACTION_TYPE] is not null", null, [EnableFolding=true])
				in
					SQL
        



    annotation PBI_ResultType = Table