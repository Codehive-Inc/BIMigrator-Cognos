{
  "id": {{ report_id | default(0) }},
  "layoutOptimization": {{ layout_optimization | default(0) }},
  "resourcePackages": [
    {
      "resourcePackage": {
        "disabled": false,
        "items": [
          {%- for resource in shared_resources %}
          {
            "name": "{{ resource.name }}",
            "path": "{{ resource.path }}",
            "type": {{ resource.type }}
          }{%- if not loop.last %},{% endif %}
          {%- endfor %}
        ],
        "name": "SharedResources",
        "type": 2
      }
    }
  ]
}
