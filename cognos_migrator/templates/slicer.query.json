{
  "Commands": [
    {
      "SemanticQueryDataShapeCommand": {
        "Query": {
          "Version": 2,
          "From": [
            {
              "Name": "{{ table_alias | default('m') }}",
              "Entity": "{{ table_name }}",
              "Type": 0
            }
          ],
          "Select": [
            {
              "Column": {
                "Expression": {
                  "SourceRef": {
                    "Source": "{{ table_alias | default('m') }}"
                  }
                },
                "Property": "{{ field_name }}"
              },
              "Name": "{{ table_name }}.{{ field_name }}",
              "NativeReferenceName": "{{ field_name }}"
            }
          ]
        },
        "Binding": {
          "Primary": {
            "Groupings": [
              {
                "Projections": [
                  0
                ]
              }
            ]
          },
          "DataReduction": {
            "DataVolume": 3,
            "Primary": {
              "Window": {}
            }
          },
          "IncludeEmptyGroups": true,
          "Version": 1
        },
        "ExecutionMetricsKind": 1
      }
    }
  ]
} 